{
  "name": "kubernaut",
  "version": "1.0.0",
  "description": "Interface between humans and k8s clusters in the ways we wish to work.",
  "main": "server/build/index.js",
  "scripts": {
    "start": "concurrently 'npm run start-server' 'npm run start-client'",
    "start-server": "npm run setup-local; nodemon server/index.js --exec babel-node --ignore client/",
    "start-client": "cd client; npm start",
    "test": "npm run test-server && npm run test-client",
    "test-server": "mocha server/test/**/*.test.js",
    "test-server-w": "mocha --watch server/test/**/*.test.js",
    "test-client": "cd client; npm test",
    "test-client-w": "cd client; npm run test-w",
    "test-client-i": "cd client; npm run test-i",
    "load-test-data": "babel-node ./server/test/bin/load-test-data.js",
    "build": "concurrently 'npm run build-server' 'npm run build-client'",
    "build-server": "rm -rf server/build && babel --out-dir server/build ./server --source-maps --copy-files --include-dotfiles",
    "build-client": "cd client; npm run build",
    "precommit": "npm run lint && npm run check-package-lock",
    "prepush": "npm test",
    "lint": "eslint .",
    "check-package-lock": "grep -qc tescloud package-lock.json; test $? -gt 0",
    "create-key": "create-key",
    "create-bearer-token": "./bin/create-bearer-token.js",
    "setup-local": "babel-node ./bin/setup-local.js",
    "install-client": "cd client; npm ci"
  },
  "author": "TES",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "chance": "^1.1.7",
    "cheerio": "^1.0.0-rc.5",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-imperative-es6": "^2.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect": "^26.6.2",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "p-limit": "^3.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "source-map-support": "^0.5.19"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.13.2",
    "async": "^3.2.0",
    "aws-sdk": "^2.820.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "bunyan": "^1.8.14",
    "chalk": "^4.1.0",
    "confabulous": "^2.0.3",
    "connect-pg-simple": "^6.2.1",
    "core-js": "^3.8.2",
    "cryptus": "^1.5.1",
    "elastic-apm-node": "^3.9.0",
    "express-session": "^1.17.1",
    "groundhog-day": "^1.2.0",
    "highwayhash": "^3.1.0",
    "hogan.js": "^3.0.2",
    "is-css-color-hex": "^0.2.0",
    "is-css-color-name": "^0.1.3",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "marv": "^3.1.0",
    "marv-pg-driver": "^4.0.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.31",
    "on-headers": "^1.0.2",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-github": "^1.1.0",
    "passport-http-bearer": "^1.0.1",
    "power-merge": "^1.5.0",
    "prepper": "^1.2.0",
    "short-hash": "^1.0.0",
    "sqb": "^3.9.2",
    "sqb-serializer-pg": "^3.1.4",
    "systemic": "^3.3.7",
    "systemic-express": "^1.1.1",
    "systemic-pg": "^3.0.0",
    "systemic-service-runner": "^1.1.0",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tes/kubernaut.git"
  },
  "bugs": {
    "url": "https://github.com/tes/kubernaut/issues"
  },
  "homepage": "https://github.com/tes/kubernaut#readme"
}
